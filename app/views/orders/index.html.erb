
<h3>Order Management</h3>

<table class="table">
  <thead>
  <th>Order ID</th>
  <th>User</th>
  <th>price</th>
  <th>status</th>
  </thead>
  <tbody>
  <% @orders.each do |order| %>
      <tr>
        <td><%= order.oid %></td>
        <td><%= order.user.name %></td>
        <td><%= order.amount %></td>
        <td><%= order.status %></td>
      </tr>
  <% end %>
  </tbody>
</table>
